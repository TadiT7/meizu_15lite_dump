    chown media system /sys/class/meizu/flash/flash1
    chmod 0666 /sys/class/meizu/flash/flash2
    chown media system /sys/class/meizu/flash/flash2
    chmod 0666 /sys/class/meizu/flash/flash3
    chown media system /sys/class/meizu/flash/flash3
    chmod 0666 /sys/class/meizu/flash/flash4
    chown media system /sys/class/meizu/flash/flash4
    chmod 0666 /sys/class/meizu/flash/flash5
    chown media system /sys/class/meizu/flash/flash5
    chmod 0666 /sys/class/meizu/flash/flash6
    chown media system /sys/class/meizu/flash/flash6
    # flash-led, add by tianhongqi@meizu.com,20171019

#Permissions for gesture {@
    chown system system /sys/class/meizu/tp/gesture_control
    chown system system /sys/class/meizu/tp/gesture_data
    chmod 0660 /sys/class/meizu/tp/gesture_control
