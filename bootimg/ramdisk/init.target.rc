# xiezhishan@stability FLYME{@
on property:persist.sys.coredump.enable=1
    mkdir /data/corefile
    chmod 777 /data/corefile
    write /proc/sys/kernel/core_pattern "/data/corefile/core-%e-%p"
    write /proc/sys/fs/suid_dumpable 1

on property:persist.sys.coredump.enable=0
    write /proc/sys/kernel/core_pattern "core"
    write /proc/sys/fs/suid_dumpable 0
# @}


